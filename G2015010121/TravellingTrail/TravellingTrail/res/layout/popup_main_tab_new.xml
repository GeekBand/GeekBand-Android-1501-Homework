<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/popup_main_tab_new_rlyt_frame"
    android:layout_width="130dp"
    android:layout_height="133dp" >
        
    <ImageView
        android:id="@+id/popup_main_tab_new_iv_locate"
        android:layout_width="43dp"
        android:layout_height="43dp"
        android:padding="10dp"
        android:src="@drawable/ic_btn_locate"
        android:scaleType="centerCrop"
        android:clickable="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/bg_item_of_tab_new" />
    
    <ImageView
        android:id="@+id/popup_main_tab_new_iv_stop"
        android:layout_width="43dp"
        android:layout_height="43dp"
        android:padding="12dp"
        android:src="@drawable/ic_btn_stop"
        android:scaleType="centerCrop"
        android:clickable="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/bg_item_of_tab_new" />
    

</RelativeLayout>
